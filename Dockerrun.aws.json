{
  "AWSEBDockerrunVersion": "1",
  "containerDefinitions": [
    {
      "name": "client",
      "image": "fayosaadi/multi-client",
      "hostname": "client",
      "essential": "false",
      "memory": "128"
    },
    {
      "name": "server",
      "image": "fayosaadi/multi-server",
      "hostname": "api",
      "essential": "false",
      "memory": "128"
    },
    {
      "name": "worker",
      "image": "fayosaadi/multi-worker",
      "essential": "false",
      "memory": "128"
    },
    {
      "name": "nginx",
      "image": "fayosaadi/multi-nginx",
      "essential": "true",
      "portMappings": [
        {
          "hostPort": "80",
          "containerPort": "80"
        }
      ],
      "links": ["client", "server"],
      "memory": "128"
    }
  ]
}
